{
    "name": "elodex/elodex",
    "description": "Provides Elasticsearch indexing functionality for Laravel Eloquent Models.",
    "keywords": ["elasticsearch", "eloquent", "laravel"],
    "license": "MIT",
    "homepage": "https://github.com/Elodex/Elodex/",
    "support": {
        "issues": "https://github.com/Elodex/Elodex/issues",
        "source": "https://github.com/Elodex/Elodex/"
    },
    "authors": [
        {
            "name": "Holger Wiedemann",
            "email": "elodex@wiedemann-it.eu"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "illuminate/database": ">=5.1 <5.3",
        "illuminate/config": ">=5.1 <5.3",
        "illuminate/container": ">=5.1 <5.3",
        "illuminate/contracts": ">=5.1 <5.3",
        "illuminate/pagination": ">=5.1 <5.3",
        "illuminate/support": ">=5.1 <5.3",
        "illuminate/console": ">=5.1 <5.3",
        "symfony/var-dumper": "2.8.*|3.0.*",
        "symfony/console": "2.8.*|3.0.*",
        "nesbot/carbon": "~1.0",
        "elasticsearch/elasticsearch": "^2.0",
        "ongr/elasticsearch-dsl": "^2.0",
        "monolog/monolog": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9",
        "phpunit/phpunit": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Elodex\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Functional/MappingModel.php",
            "tests/Integration/IndexedModel.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master" : "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
